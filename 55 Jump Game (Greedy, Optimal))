class Solution {
    public boolean canJump(int[] nums) {
        int max_reach=0;
        for(int i=0;i<nums.length;i++){
            if(i>max_reach){
                return false;
            }
            int contend=i+nums[i];
            if(contend>max_reach){
                max_reach=contend;
            }
        }
        return true;
    }
}
