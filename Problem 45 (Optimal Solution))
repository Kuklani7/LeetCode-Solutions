class Solution {
    public int jump(int[] nums) {
        int jumps=0;
        int runway=0;
        int reach=0;
        for(int i=0;i<nums.length-1;i++){
            int now=nums[i]+i;
            if(now>reach){
                reach=now;
            }
            if(i==runway){
                jumps++;
                runway=reach;
            }
        }
        return jumps;
    }
}
